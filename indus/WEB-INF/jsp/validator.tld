	<tag>	
	    <name>equals</name>
    	    <tagclass>com.taglib.util.CompareString</tagclass>
    	    <bodycontent>JSP</bodycontent>
    	    <info>compare the two strings</info>
    	<attribute>
      		<name>name</name>
      		<required>true</required>
      		<rtexprvalue>false</rtexprvalue>
    	</attribute>
    	<attribute>
      		<name>value</name>
      		<required>false</required>
      		<rtexprvalue>false</rtexprvalue>
    	</attribute>
    	<attribute>
      		<name>match</name>
      		<required>true</required>
      		<rtexprvalue>true</rtexprvalue>
    	</attribute>
    </tag>

</taglib>
