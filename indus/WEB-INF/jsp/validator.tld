<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
  PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<!-- a tag library descriptor -->

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>validator</shortname>
	<uri>http://cl/taglibs/validator</uri>
	<info>cl taglibs</info>

	<tag>
		<name>
			passwordvalid
		</name>
		<tagclass>
			com.taglib.util.IsValidPassword
		</tagclass>
		<bodycontent>
			JSP
		</bodycontent>
		<attribute>
			<name>
				password
			</name>
			<required>
				true
			</required>
			<rtexprvalue>
				true
			</rtexprvalue>
		</attribute>
		<attribute>
			<name>
				value
			</name>
			<required>
				false
			</required>
		</attribute>
		
	</tag>
	
